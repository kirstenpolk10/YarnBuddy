{
  "expo": {
    "name": "MyFirstApp",
    "slug": "myfirstapp",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kirstenp.myfirstapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./public/favicon.png",
      "manifest": {
        "name": "My First App",
        "short_name": "FirstApp",
        "start_url": ".",
        "display": "standalone",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "icons": [
          {
            "src": "./public/favicon-3.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./public/favicon-3.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },

    "extra": {
      "eas": {
        "projectId": "ca563341-5c2a-438b-8b80-0e4a3888c0e8"
      }
    }
  }
}
